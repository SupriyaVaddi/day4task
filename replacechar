public class MyFirstJava2 { 

String myReplace(String data,char find,char replace)
{
char[] ar= data.toCharArray();

for(int i=0;i<ar.length;i++)
{

if(ar[i]==find)
{
ar[i]=replace;
break;
}

}

String val=new String(ar);
return  val;
}


String myReplace1(String data,char find,char replace)
{
char[] ar= data.toCharArray();

for(int i=ar.length-1;i>=0;i--)
{

if(ar[i]==find)
{
ar[i]=replace;
break;
}
}

String val=new String(ar);
return  val;
}

